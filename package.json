    "@angular/language-service": "^7.1.1",
    "@angularclass/hmr": "^2.1.3",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@types/express": "^4.16.0",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "css-loader": "^0.28.11",
    "extract-loader": "^2.0.1",
